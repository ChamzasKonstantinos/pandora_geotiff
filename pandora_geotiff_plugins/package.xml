<?xml version="1.0"?>
<package>
  <name>pandora_geotiff_plugins</name>
  <version>0.0.1</version>
  <description>
    pandora_geotiff_plugins contains plugins that extend geotiff maps generated by pandora_geotiff.
  </description>


  <maintainer email="chamzask@gmail.com">Chamzas Konstantinos</maintainer>
 
  <license>BSD</license>

  <author email="chamzask@gmail.com">Chamzas Konstantinos</author>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>pandora_geotiff</build_depend>

  <run_depend>pandora_geotiff</run_depend>
  
  <build_depend>polygon_interface_package</build_depend>
  <run_depend>polygon_interface_package</run_depend>


  <export>
   
    <pandora_geotiff plugin="${prefix}/pandora_geotiff_plugins.xml" />

  </export>
</package>
