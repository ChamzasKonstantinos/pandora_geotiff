<?xml version="1.0"?>
<package>
  <name>pandora_geotiff_plugins</name>
  <version>0.0.1</version>
  <description>
    pandora_geotiff_plugins contains plugins that extend geotiff maps generated by pandora_geotiff.
  </description>

  <maintainer email="chamzask@gmail.com">Chamzas Konstantinos</maintainer>

  <license>BSD</license>

  <author>Chamzas Konstantinos</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>pluginlib</build_depend>
  <build_depend>nav_msgs</build_depend>
  <build_depend>tf</build_depend>
  <build_depend>pandora_geotiff</build_depend>
  <build_depend>pandora_data_fusion_msgs</build_depend>

  <run_depend>pluginlib</run_depend>
  <run_depend>nav_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>pandora_geotiff</run_depend>
  <run_depend>pandora_data_fusion_msgs</run_depend>

  <export>
    <pandora_geotiff plugin="${prefix}/pandora_geotiff_plugins.xml" />
  </export>
</package>
